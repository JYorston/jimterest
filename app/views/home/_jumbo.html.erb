<div class="jumbotron center">
    <h1>Welcome to Jimterest</h1>
    <p>Sign up for a free account</p>
        <% if user_signed_in? %>
            <%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'btn btn-primary' %>
            <%= link_to "Account", edit_user_registration_path, class: 'btn btn-default' %>
            <%= link_to "Feed", pins_path, class: 'btn btn-default' %>
        <% else %>
            <%= link_to "Login", new_user_session_path, class: 'btn btn-default' %>
            <%= link_to "Join", new_user_registration_path, class: 'btn btn-primary' %>
        <% end %>
</div>